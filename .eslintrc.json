{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": ["error", 4],
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "max-len": ["error", { "code": 120 }],
        "no-useless-catch": 0
    }
}