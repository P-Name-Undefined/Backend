{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "no-console": 0,
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "no-useless-catch": 0,
        "class-methods-use-this": 0
    }
}